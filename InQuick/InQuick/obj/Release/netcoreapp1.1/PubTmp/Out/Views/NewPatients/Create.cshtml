@model InQuick.Models.NewPatient

@{
    ViewData["Title"] = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Create New Patient Registration</h2>

<label class="btn btn-lg btn-in-quick">
    Open XML File
    <input id="open-xml-btn" type="file" />
</label>

<form asp-action="Create">

    <div asp-validation-summary="ModelOnly" class="text-danger"></div>

    <fieldset>

        <!-- Date of Consultation -->
        <div class="col-md-6 form-group form-group-lg">
            <label asp-for="ConsultationDate" class="col-xs-4 control-label"></label>
            <div class="col-xs-8">
                <input type="date" asp-for="ConsultationDate" class="form-control" />
                <span asp-validation-for="ConsultationDate" class="text-danger"></span>
            </div>
        </div>

        <!-- Name of Doctor -->
        <div class="col-md-6 form-group form-group-lg">
            <label asp-for="DoctorsName" class="col-xs-4 control-label"></label>
            <div class="col-xs-8">
                <input asp-for="DoctorsName" class="form-control" />
                <span asp-validation-for="DoctorsName" class="text-danger"></span>
            </div>
        </div>

        <!-- Referred By -->
        <div class="col-md-6 form-group form-group-lg">
            <label asp-for="ReferredBy" class="col-xs-4 control-label"></label>
            <div class="col-xs-8">
                <input asp-for="ReferredBy" class="form-control" />
                <span asp-validation-for="ReferredBy" class="text-danger"></span>
            </div>
        </div>

        <!-- Case Type -->
        <div class="col-md-6 form-group form-group-lg">
            <label asp-for="CaseType" class="col-xs-4 control-label"></label>
            <div class="col-xs-8">
                <input asp-for="CaseType" class="form-control" />
                <span asp-validation-for="CaseType" class="text-danger"></span>
            </div>
        </div>

        <!-- Details of Injury or Illness -->
        <div class="col-md-6 form-group form-group-lg">
            <label asp-for="IllnessDetails" class="col-xs-4 control-label"></label>
            <div class="col-xs-8">
                <textarea asp-for="IllnessDetails" class="form-control" rows="3" placeholder="Include date, location, and other details."></textarea>
                <span asp-validation-for="IllnessDetails" class="text-danger"></span>
            </div>
        </div>

        <!-- Details of Treatment Administered -->
        <div class="col-md-6 form-group form-group-lg">
            <label asp-for="TreatmentDetails" class="col-xs-4 control-label"></label>
            <div class="col-xs-8">
                <textarea asp-for="TreatmentDetails" class="form-control" rows="3" placeholder="?"></textarea>
                <span asp-validation-for="TreatmentDetails" class="text-danger"></span>
            </div>
        </div>

    </fieldset>

    <fieldset>

        <!-- Accordion -->
        <div class="panel-group" id="new-patient-accordion" role="tablist" aria-multiselectable="true">

            <!-- Registration Details -->
            <div class="panel panel-default">

                <div class="panel-heading" role="tab" id="registration-heading" data-toggle="collapse" data-parent="#new-patient-accordion" href="#registration-details">
